{
  "name": "markdown-leaner-newline",
  "displayName": "markdown-leaner-newline",
  "description": "Just insert markdown symbol on newline without incrementing.",
  "publisher": "awtnb",
  "icon": "images/logo.png",
  "repository": {
    "url": "https://github.com/AWtnb/vscode-markdown-leaner-newline"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other",
    "Keymaps"
  ],
  "keywords": [
    "Markdown"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdown-leaner-newline.execute",
        "title": "newline",
        "category": "markdown"
      }
    ],
    "keybindings": [
      {
        "command": "markdown-leaner-newline.execute",
        "key": "enter",
        "when": "editorLangId == 'markdown' && editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}

